{
    "name": "pyLoad",
    "id": "pyload",
    "packaging_format": 1,
    "description": {
        "en": "Download Manager written in pure Python."
    },
    "version": "0.4.20~ynh1",
    "url": "https://pyload.net/",
    "upstream": {
        "license": "free",
        "website": "https://pyload.net",
        "admindoc": "https://github.com/pyload/pyload/wiki",
        "code": "https://github.com/pyload/pyload"
    },
    "license": "free",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/pyload",
                "default": "/pyload"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
